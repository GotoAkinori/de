{
    "$schema": "http://json-schema.org/draft-04/schema",
    "properties": {
        "commandUrl": {
            "description": "URL of command server",
            "type": "string"
        },
        "staticUrl": {
            "description": "URL of static file server",
            "type": "string"
        },
        "port": {
            "description": "Port number of the server",
            "type": "number"
        },
        "clientRoot": {
            "description": "Root path of client application",
            "type": "string"
        }
    },
    "required": [
        "url",
        "clientRoot"
    ]
}